{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": false,
        "baseUrl": "./src",
        "paths": {
            "*": [
                "*"
            ]
        },
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "react-native",
        "useUnknownInCatchVariables": false
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "**/*.d.ts"
    ]
}